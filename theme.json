{
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#444444",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#003c71",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#71003c",
					"name": "Accent",
					"slug": "accent"
				},
				{
					"color": "#0057a4",
					"name": "Hover",
					"slug": "hover"
				}
			]
		},
		"layout": {
			"contentSize": "50rem",
			"wideSize": "62.5rem"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"fluid": false,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "\"Noto Serif\"",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/noto-serif/noto-serif-400-normal.woff2"
							]
						},
						{
							"fontFamily": "\"Noto Serif\"",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/noto-serif/noto-serif-700-normal.woff2"
							]
						}
					],
					"fontFamily": "\"Noto Serif\", serif",
					"name": "Noto Serif",
					"slug": "noto-serif"
				},
				{
					"fontFace": [
						{
							"fontFamily": "\"Noto Sans\"",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/noto-sans/noto-sans-400-normal.woff2"
							]
						}
					],
					"fontFamily": "\"Noto Sans\", sans-serif",
					"name": "Noto Sans",
					"slug": "noto-sans"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "1.13rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "1.5rem",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "2rem",
					"slug": "x-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "3rem"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "2.5rem"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "2.25rem"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "2rem"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "1.75rem"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "1.5rem"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--noto-sans)",
					"fontStyle": "normal",
					"fontWeight": "400"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--hover)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"spacing": {
			"padding": {
				"bottom": "0px",
				"left": "1px",
				"right": "1px",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--noto-serif)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.5"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json"
}